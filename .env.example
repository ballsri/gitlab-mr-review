# AI Provider API Keys (at least one required)
GOOGLE_API_KEY=your-google-gemini-api-key-here
ANTHROPIC_API_KEY=your-anthropic-claude-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# GitLab Configuration (required for testing)
GITLAB_TOKEN=your-gitlab-token-here
GITLAB_URL=https://gitlab.com

# Test Configuration (required for local testing)
PROJECT_ID=your-project-id
MR_IID=your-mr-iid

# AI Model Selection (optional, defaults to gemini)
AI_MODEL=gemini

#  Specific model name
MODEL_NAME=gemini-2.5-flash 

# Cloud Provider Configuration (for deployment)
# GCP
GCP_REGION=us-central1

# AWS
AWS_REGION=us-east-1
AWS_LAMBDA_ROLE_ARN=arn:aws:iam::ACCOUNT_ID:role/ROLE_NAME

# Function Configuration (optional)
FUNCTION_NAME=gitlab-mr-review
MEMORY=512
TIMEOUT=540
